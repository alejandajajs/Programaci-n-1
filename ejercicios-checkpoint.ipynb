    "# Ejercicio 5: Tabla de Multiplicar\n",
    "numero = int(input(\"Ingresa un número: \"))\n",
    "\n",
    "for i in range(1, 11):\n",
    "    print(f\"{numero} x {i} = {numero * i}\")\n",
    "\n"
   ]
  },
