   "source": [
    "# Ejercicio 6: Pirámide de Asteriscos\n",
    "n = int(input(\"Ingresa el número de niveles para la pirámide: \"))\n",
    "\n",
    "for i in range(1, n + 1):\n",
    "    # Imprimir espacios\n",
    "    print(\" \" * (n - i) + \"*\" * (2 * i - 1))\n"
   ]
